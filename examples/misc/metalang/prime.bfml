// トランスパイル時に素数を列挙する

open import "std/std.bfml"
open import "./puti.bfml"

let is_prime = fun n ->
  fix
    (fun loop -> fun i ->
      if i * i <= n then
        if n mod i = 0 then false
        else loop (i + 1)
      else true)
    2

let main = [
  $build {sp}
  + sp ' '

  *fix
    (fun loop -> fun i ->
      if i < 100 then [
        *(if is_prime i then [
            *puti i
            . sp
          ] else [] )
        *loop (i + 1)
      ] else [])
    2
]