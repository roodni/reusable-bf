open import "std/std.bfml"

let main = $

$build { x }
, x

*Ctrl:gen_switch x
  ( ('+', [ *IO:gen_puts "INC" ]);
    ('-', [ *IO:gen_puts "DEC" ]);
    ('.', [ *IO:gen_puts "PUT" ]);
    (',', [ *IO:gen_puts "GET" ]);
    ('[', [ *IO:gen_puts "WHILE" ]);
    (']', [ *IO:gen_puts "WEND" ]);
    ('>', [ *IO:gen_puts "SHR" ]);
    ('<', [ *IO:gen_puts "SHL" ]);
  )
  [ *IO:gen_puts "OTHER" ]