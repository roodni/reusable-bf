open import "std/std.bfml"

let main = $

$build { x }
, x

*gen_switch x
  ( ('+', [ *gen_puts "INC" ]);
    ('-', [ *gen_puts "DEC" ]);
    ('.', [ *gen_puts "PUT" ]);
    (',', [ *gen_puts "GET" ]);
    ('[', [ *gen_puts "WHILE" ]);
    (']', [ *gen_puts "WEND" ]);
    ('>', [ *gen_puts "SHR" ]);
    ('<', [ *gen_puts "SHL" ]);
  )
  [ *gen_puts "OTHER" ]