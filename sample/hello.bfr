// Hello World!

// 不動点コンビネータ (型検査は未実装)
let fix f =
  let g x = f (fun y -> x x y) in
  g g

// 文字列 (=整数のリスト) を引数に取り、文字列を出力する文列を返す
let gen_puts l = [
  $alloc { c }
  *fix
    (fun loop prev l -> match l with
      | {} -> []
      | hd :: tl -> [
          + c (hd - prev)
          . c
          *loop hd tl
        ]
    ) 0 l
]

codegen [ *gen_puts "Hello World!\n" ]